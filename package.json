    "koa-router": "12.0.0",
    "localenv": "0.2.2",
    "optimist": "0.6.1",
    "pump": "3.0.0",
    "tldjs": "2.3.1"
  },
  "devDependencies": {
    "esbuild": "0.19.2",
    "mocha": "10.2.0",
    "node-dev": "8.0.0",
    "supertest": "6.3.3",
    "ws": "8.14.1"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
